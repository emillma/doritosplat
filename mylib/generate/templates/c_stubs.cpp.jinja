#include <optix_types.h>
#include <optix_stubs.h>
#include <optix_function_table_definition.h>

#include "my_types.h"

#include <torch/extension.h>

PYBIND11_MODULE(c_stubs, m)
{
    {% for s in stubs%}
    {% if s.return_type != "void" %}
    m.def("{{s.name}}", []({{s.c_py_arg_string()}}) { return {{s.name}}({{s.c_py2c_arg_string()}});});
    {% endif %}
    {% endfor %}
}
    

